{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#assets/*": [
        "assets/*"
      ],
      "#auth/*": [
        "src/modules/auth/*"
      ],
      "#playground/*": [
        "src/modules/playground/*"
      ]
    }
  },
  "include": [
    "**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
  ]
}
